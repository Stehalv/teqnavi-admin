{
  "include": ["**/*.ts", "**/*.tsx"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./app/*"]
    },
    "jsx": "react-jsx",
    "moduleResolution": "node16",
    "allowJs": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "module": "Node16",
    "target": "ES2020"
  }
}
